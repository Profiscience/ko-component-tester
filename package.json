{
  "name": "ko-component-tester",
  "version": "1.0.0",
  "description": "automated testing for KnockoutJS components",
  "repository": "barsh/ko-component-tester",
  "main": "dist/ko-component-tester.js",
  "scripts": {
    "build": "webpack && uglifyjs dist/ko-component-tester.js -o dist/ko-component-tester.min.js",
    "watch": "webpack --watch",
    "prepublish": "npm run build",
    "stage-dist": "git add dist",
    "test": "karma start karma.travis.js",
    "test:watch": "karma start"
  },
  "precommit": [
    "build",
    "stage-dist"
  ],
  "author": "Michael Barshinger",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "codeclimate-test-reporter": "^0.3.0",
    "chai": "^3.5.0",
    "isparta-loader": "^2.0.0",
    "jquery": "^2.2.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "knockout": "^3.4.0",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "pre-commit": "^1.1.2",
    "sinon": "^2.0.0-pre",
    "uglify-js": "^2.4.24",
    "webpack": "^1.13.0"
  },
  "peerDependencies": {
    "jquery": "^2.2.2",
    "knockout": "^3.4.0"
  }
}
